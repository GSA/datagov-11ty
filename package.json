{
  "name": "11ty-uswds-template",
  "version": "0.0.1",
  "description": "A 11ty UWDS template.",
  "main": "index.js",
  "scripts": {
    "build": "npm run assets:build && npx @11ty/eleventy",
    "assets:autoprefix": "postcss _site/assets/styles/*.css -r --use autoprefixer",
    "assets:build": "node ./config/buildAssets && npm run assets:autoprefix",
    "assets:clean": "rimraf _site/assets",
    "assets:refresh": "npm run assets:clean && npm run assets:build",
    "assets:watch": "chokidar 'js/**' 'styles/**' -c 'npm run assets:refresh' --polling",
    "clean": "rimraf _site",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "cypress:gh": "npx start-server-and-test start 8080 cypress:run",
    "dev": "npm run clean && npm-run-all -p dev:assets dev:serve",
    "dev:clean": "npm run clean && npm run dev",
    "dev:assets": "npm run assets:refresh && npm run assets:watch",
    "dev:debug": "DEBUG=* npx @11ty/eleventy --serve --watch",
    "dev:serve": "npx @11ty/eleventy --serve --watch",
    "dev:cms": "npx netlify-cms-proxy-server",
    "federalist": "npm run build",
    "pa11y-ci": "pa11y-ci --sitemap http://localhost:8080/sitemap.xml --sitemap-exclude \"(/admin|/contact)\"  --sitemap-find \"^/\" --sitemap-replace \"http://localhost:8080/\"",
    "pa11y-ci:gh": "npx start-server-and-test start 8080 pa11y-ci",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx @11ty/eleventy --serve",
    "test": "npm run woke; npm run cypress:run; npm run pa11y-ci",
    "woke": "woke"
  },
  "author": "",
  "license": "CC0-1.0",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.1.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@types/node": "^17.0.25",
    "autoprefixer": "^10.4.2",
    "chokidar-cli": "^3.0.0",
    "cypress": "^9.7.0",
    "eleventy-plugin-svg-sprite": "^1.2.2",
    "esbuild": "^0.14.25",
    "esbuild-sass-plugin": "^2.2.4",
    "js-yaml": "^4.1.0",
    "luxon": "^2.3.1",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "netlify-cms-proxy-server": "^1.3.23",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^3.0.1",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "netlify-cms": "^2.10.187",
    "turndown": "^7.1.1",
    "uswds": "^2.13.1"
  }
}
